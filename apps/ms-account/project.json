{
  "name": "ms-account",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ms-account/src",
  "projectType": "application",
  "prefix": "ms-account",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/ms-account",
        "main": "apps/ms-account/src/main.ts",
        "tsConfig": "apps/ms-account/tsconfig.app.json",
        "assets": ["apps/ms-account/src/assets"],
        "target": "node",
        "compiler": "tsc"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/ms-account/src/environments/environment.ts",
              "with": "apps/ms-account/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/node:node",
      "options": {
        "buildTarget": "ms-account:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/ms-account/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/ms-account"],
      "options": {
        "jestConfig": "apps/ms-account/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
